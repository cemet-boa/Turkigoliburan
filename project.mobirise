        {
          "_name": "features9",
          "_customHTML": "<section class=\"firmm4_features9 carousel slide testimonials-slider\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"BootstrapCarouselSwipe,mbr-testimonials-slider\" data-interval=\"false\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" name=\"fullWidth\" title=\"Full Width\">\n        <input type=\"range\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" inline>\n        <input type=\"range\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" inline>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Card Title\" name=\"showCardTitle\">\n        <input type=\"checkbox\" title=\"Description\" name=\"showDesk\">\n        <header>Slides</header>\n        <input type=\"range\" inline title=\"Items\" name=\"testimonialsSlides\" min=\"1\" max=\"10\" step=\"1\" value=\"1\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"showAutoplay\" checked>\n        <input type=\"range\" inline title=\"Interval\" name=\"interval\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showAutoplay\">\n        <input type=\"color\" title=\"Indicators\" name=\"colorCircle\" value=\"#ffb18a\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/firmm4/components/_images/background/background.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"text-center\" mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"title_block\" mbr-if=\"showTitle || showSubtitle\">\n                <h4 class=\"mbr-section-subtitle mbr-fonts-style align-center\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                    Galeri <span style=\"font-weight: normal;\">Kami</span></h4>\n                <h3 class=\"mbr-section-title mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                    Follow Instagram <a href=\"https://www.instagram.com/turkigoliburan.id/\" class=\"text-primary\" target=\"_blank\"><b>@turkigoliburan.id&nbsp;</b></a></h3>\n            </div>\n\n        <div class=\"carousel slide\" role=\"listbox\" data-pause=\"true\" data-keyboard=\"false\" data-ride=\"{{showAutoplay ? 'carousel' : 'false'}}\" data-interval=\"{{showAutoplay ? interval*1000 : 'false'}}\">\n            <div mbr-list mbr-list-grow=\"testimonialsSlides\" class=\"carousel-inner\">\n                <div class=\"carousel-item\">\n                    <div class=\"row justify-content-center\">\n                        <div class=\"col-lg-4 col-md-4 col-sm-10 col-10 user\">\n                            <div class=\"user_image\">\n                                <img src=\"@PROJECT_PATH@/assets/images/test-3-670x560.png\" alt>\n                            </div>\n                            <h5 class=\"card_title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card_title\"><b><a href=\"index.html\" class=\"text-secondary\">Design</a></b></h5>\n                            <p class=\"user_descr mbr-fonts-style\" mbr-if=\"showDesk\" mbr-theme-style=\"display-4\" data-app-selector=\".user_descr\">Design, Branding</p>\n                        </div>\n\n                        <div class=\"col-lg-4 col-md-4 col-sm-10 col-10 user\">\n                            <div class=\"user_image\">\n                                <img src=\"@PROJECT_PATH@/assets/images/test-670x560.png\" alt>\n                            </div>\n                            <h5 class=\"card_title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card_title\"><b><a href=\"index.html\" class=\"text-secondary\">Promotion</a></b></h5>\n                            <p class=\"user_descr mbr-fonts-style\" mbr-if=\"showDesk\" mbr-theme-style=\"display-4\" data-app-selector=\".user_descr\">Advertising, Agency</p>\n                        </div>\n\n                        <div class=\"col-lg-4 col-md-4 col-sm-10 col-10 user\">\n                            <div class=\"user_image\">\n                                <img src=\"@PROJECT_PATH@/assets/images/test-2-670x560.png\" alt>\n                            </div>\n                            <h5 class=\"card_title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card_title\"><b><a href=\"index.html\" class=\"text-secondary\">Optimization</a></b></h5>\n                            <p class=\"user_descr mbr-fonts-style\" mbr-if=\"showDesk\" mbr-theme-style=\"display-4\" data-app-selector=\".user_descr\">Innovative, Branding</p>\n                        </div>\n                    </div>\n                    \n                </div>\n     